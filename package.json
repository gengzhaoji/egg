{
  "name": "egg_vue",
  "version": "1.0.0",
  "description": "egg框架和vue框架的结合使用demo",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.11.0",
    "egg-session": "^3.1.0",
    "egg-socket.io": "^4.1.6",
    "egg-static": "^2.1.0",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --sticky --title=egg-server-egg_vue",
    "stop": "egg-scripts stop --title=egg-server-egg_vue",
    "debug": "egg-bin debug",
    "dev": "egg-bin dev --sticky",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
